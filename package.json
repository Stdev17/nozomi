{
  "name": "nozomi",
  "version": "1.0.0",
  "description": "",
  "main": "src/main.js",
  "scripts": {
    "all": "cross-env npm run clean && npm run generate && npm run publish",
    "clean": "npm run ts-node scripts/clean.ts",
    "clean-output": "npm run ts-node scripts/clean-output.ts",
    "clean-generated": "npm run ts-node scripts/clean-generated.ts",
    "generate": "cross-env NOZOMI_NAMESPACE=Nozomi.Dev TARGET_FILES=server/src/index.ts,dev/message-handler.ts npm run ts-node src/main.ts",
    "publish": "cross-env NOZOMI_NAMESPACE=Nozomi.Dev npm run ts-node scripts/publish.ts",
    "test-jest": "jest",
    "test-api": "npm run ts-node scripts/test-api.ts",
    "ts-node": "ts-node -r tsconfig-paths/register --transpile-only",
    "test-watch": "jest --watch",
    "lint": "tslint -p ./tsconfig.json",
    "spawn": "cross-env NOZOMI_NAMESPACE=ASDF npm run ts-node spawn.ts",
    "build": "tsc && cp -r ./src/template ./dist/src/template",
    "check:watch": "tsc -w --skipLibCheck"
  },
  "author": "5minlab",
  "license": "ISC",
  "devDependencies": {
    "@types/callsite": "^1.0.30",
    "@types/findup-sync": "^2.0.1",
    "@types/fs-extra": "^7.0.0",
    "@types/glob": "^7.1.1",
    "@types/iconv-lite": "0.0.1",
    "@types/jest": "^24.0.13",
    "@types/lodash": "^4.14.126",
    "@types/mustache": "^0.8.32",
    "@types/node": "^12.0.2",
    "@types/relaxed-json": "^1.0.0",
    "cross-env": "^5.2.0",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "yup": "^0.27.0"
  },
  "dependencies": {
    "callsite": "^1.0.0",
    "findup-sync": "^3.0.0",
    "fs-extra": "^8.0.1",
    "glob": "^7.1.4",
    "iconv-lite": "^0.4.24",
    "lodash": "^4.17.11",
    "mustache": "^3.0.1",
    "relaxed-json": "^1.0.3",
    "source-map-support": "^0.5.12",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.4.5"
  }
}
